{
  "version": 2,
  "name": "markdownzhuanhuan-v015",
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/product.html",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}